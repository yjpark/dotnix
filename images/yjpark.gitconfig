[alias]
	br = "branch"
	ci = "commit"
	co = "checkout"
	lg = "log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
	st = "status"

[init]
	defaultBranch = "main"

[pull]
	rebase = "false"

[push]
	default = "current"

[user]
	email = "yjpark@gmail.com"
	name = "YJ Park"
